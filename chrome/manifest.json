{
    "name": "ad ade",
    "version": "0.1",
    "manifest_version": 2,
    "description": "Lass die Werbung aus deinem Browser verschwinden und bekomme stattdessen t√§glich verschiedene Artikel, Bilder oder Videos.",
    "icons": {
        "16": "assets/icon-16.png",
        "48": "assets/icon-48.png",
        "128": "assets/icon-128.png"
    },
    "homepage_url": "http://ad-ade.de/",
    "browser_action": {
        "default_icon": "assets/icon-19.png"
    },
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "content_scripts": [
        {
            "matches": ["http://*/*"],
            "css": ["content.css", "fancybox/jquery.fancybox.css"],
            "js": ["jquery.min.js", "fancybox/jquery.fancybox.pack.js", "adreplacer.js", "content.js"]
        }
      ],
    "permissions": [
        "tabs",
        "http://*/*"
    ],
    "web_accessible_resources": [
    ],
    "incognito": "spanning"
}